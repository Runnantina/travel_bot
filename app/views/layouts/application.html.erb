<!DOCTYPE html>
<html>
  <head>
    <title>TravelBot</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <%= yield %>

      <%= form_tag '/logout', method:"post" do %>
      <%= submit_tag 'Log Out'%>
      <% end %>
    </div>


  </body>
</html>
